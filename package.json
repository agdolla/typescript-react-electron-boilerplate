{
  "name": "typescript-react-webpack",
  "version": "0.0.0",
  "description": "typescript / react / redux / electron boilerplate",
  "repository": {
    "type": "git",
    "url": "https://github.com/xwvvvvwx/typescript-react-electron-boilerplate"
  },
  "license": "Unlicense",
  "main": "main.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development electron .",
    "server": "node dev-server.js",
    "lint-css": "stylelint \"src/**/*.css\"",
    "lint-ts": "tslint \"src/**/*.ts?(x)\"",
    "lint": "npm run lint-css && npm run lint-ts",
    "test": "cross-env NODE_PATH=./src mocha --opts mocha.opts",
    "test:watch": "npm run test -- --watch",
    "prepackage": "cross-env NODE_ENV=production webpack",
    "build-win": "npm run prepackage && electron-packager . --out=build/package/ --prune --overwrite --platform=win32 --arch=x64",
    "build-mac": "npm run prepackage && electron-packager . --out=build/package/ --prune --overwrite --platform=darwin --arch=x64",
    "build-linux": "npm run prepackage && electron-packager . --out=build/package/ --prune --overwrite --platform=linux --arch=x64",
    "build-all": "npm run prepackage && electron-packager . --out=build/package/ --prune --overwrite --platform=win32,darwin,linux --arch=x64",
    "archive-builds": "tar -zcf builds.tar.gz build/package"
  },
  "devDependencies": {
    "@types/chai": "^3.4.32",
    "@types/enzyme": "^2.4.32",
    "@types/expect": "^1.13.29",
    "@types/mocha": "^2.2.31",
    "@types/node": "^6.0.38",
    "@types/react": "^0.14.33",
    "@types/react-dom": "^0.14.16",
    "@types/react-redux": "^4.4.31",
    "awesome-typescript-loader": "^2.2.4",
    "chai": "^3.5.0",
    "cross-env": "^2.0.0",
    "css-loader": "^0.25.0",
    "devtron": "^1.3.0",
    "electron": "^1.3.4",
    "electron-debug": "^1.0.1",
    "electron-devtools-installer": "^2.0.1",
    "electron-packager": "^7.7.0",
    "enzyme": "^2.4.1",
    "express": "^4.14.0",
    "html-webpack-plugin": "^2.22.0",
    "ignore-styles": "^5.0.1",
    "json-loader": "^0.5.4",
    "mocha": "^3.0.2",
    "postcss-cssnext": "^2.8.0",
    "postcss-loader": "^0.13.0",
    "postcss-simple-vars": "^3.0.0",
    "react-addons-test-utils": "^15.3.1",
    "style-loader": "^0.13.1",
    "stylelint": "^7.4.0",
    "stylelint-config-standard": "^13.0.2",
    "ts-node": "^1.3.0",
    "tslint": "^3.14.0",
    "typescript": "^2.0.2",
    "webpack": "^1.13.2",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-fail-plugin": "^1.0.5",
    "webpack-hot-middleware": "^2.12.2",
    "webpack-merge": "^0.14.1",
    "webpack-notifier": "^1.4.1"
  },
  "dependencies": {
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2"
  }
}
