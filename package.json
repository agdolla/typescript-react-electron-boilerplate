{
  "name": "typescript-react-webpack",
  "version": "0.0.1",
  "main": "main.js",
  "scripts": {
    "server": "rimraf package && node server.js",
    "dev": "cross-env NODE_ENV=development electron .",
    "prepackage": "rimraf package && cross-env NODE_ENV=production webpack -p --config webpack.config.prod.js",
    "build-win": "npm run prepackage && electron-packager . --out=package/ --prune --overwrite --platform=win32 --arch=x64",
    "build-mac": "npm run prepackage && electron-packager . --out=package/ --prune --overwrite --platform=darwin --arch=x64",
    "build-linux": "npm run prepackage && electron-packager . --out=package/ --prune --overwrite --platform=linux --arch=x64",
    "build-unix": "npm run prepackage && electron-packager . --out=package/ --prune --overwrite --platform=linux,darwin --arch=x64",
    "build-all": "npm run prepackage && electron-packager . --out=package/ --prune --overwrite --platform=win32,darwin,linux --arch=x64",
    "archive-builds": "tar -zcvf builds.tar.gz package",
    "lint": "tslint \"src/**/*.ts?(x)\""
  },
  "devDependencies": {
    "@types/node": "^6.0.38",
    "@types/react": "^0.14.33",
    "@types/react-dom": "^0.14.16",
    "@types/react-redux": "^4.4.31",
    "@types/redux": "^3.5.28",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "cross-env": "^2.0.0",
    "electron": "^1.3.4",
    "electron-devtools-installer": "^2.0.1",
    "electron-packager": "^7.7.0",
    "html-webpack-plugin": "^2.22.0",
    "json-loader": "^0.5.4",
    "react-hot-loader": "^3.0.0-beta.2",
    "rimraf": "^2.5.4",
    "ts-loader": "^0.8.2",
    "tslint": "^3.14.0",
    "typescript": "^2.1.0-dev.20160826",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.15.0",
    "webpack-notifier": "^1.4.1"
  },
  "dependencies": {
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2"
  }
}
